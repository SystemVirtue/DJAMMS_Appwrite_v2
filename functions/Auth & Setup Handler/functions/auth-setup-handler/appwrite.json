{
  "$id": "auth-setup-handler",
  "name": "Auth & Setup Handler",
  "runtime": "node-18.0",
  "execute": ["any"],
  "events": [
    "users.*.sessions.*.create"
  ],
  "schedule": "",
  "timeout": 30,
  "enabled": true,
  "logging": true,
  "entrypoint": "src/main.js",
  "commands": "npm install",
  "ignore": [
    "node_modules",
    ".npm",
    ".git",
    "*.log",
    "*.md"
  ],
  "variables": {}
}