{
  "$id": "scheduler-maintenance-agent",
  "name": "Scheduler & Maintenance Agent",
  "runtime": "node-18.0",
  "execute": ["any"],
  "events": [],
  "schedule": "0 */6 * * *",
  "timeout": 300,
  "enabled": true,
  "logging": true,
  "entrypoint": "src/main.js",
  "commands": "npm install",
  "ignore": [
    "node_modules",
    ".npm",
    ".git",
    "*.log",
    "*.md"
  ],
  "variables": {}
}